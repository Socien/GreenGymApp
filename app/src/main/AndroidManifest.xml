        <activity android:name=".Method_UpperMuscle" />
        <activity android:name=".Method_WalkingAir" />
        <activity android:name=".Method_Surfing" />
        <activity android:name=".Method_TurnWaist" />
        <activity android:name=".Method_Run" />
        <activity android:name=".Search_Map" />
        <activity android:name=".Search_byEquip" />
        <activity android:name=".Search" />
        <activity android:name=".Rate_Statistic" />
        <activity android:name=".Rate" />
        <activity android:name=".Method" />
        <activity android:name=".MainActivity" />
        <activity android:name=".IntroActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>